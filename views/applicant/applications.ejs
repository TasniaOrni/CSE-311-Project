<%- include('includes/head') %>

<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h3 class="mt-4 mb-3">Applied Jobs</h3>

      <!-- for each loop -->
      <% if(data.jobs.length> 0 ) { data.jobs.forEach((job, index) => { %>
      <%- include('modules/appliedJobCard', {job, index}) %>
      <% }) } else { %>
      <h5 class="text-muted">No where you applied yet.</h5>
      <% } %>
    </div>
  </div>
</div>

<%- include('includes/foot') %>